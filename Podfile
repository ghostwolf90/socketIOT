# platform :ios, '9.0'

def install_pods
    pod 'CocoaAsyncSocket' 
end


target 'socketIOT' do
  use_frameworks!
  install_pods
end

target 'socketIOTTests' do
    use_frameworks!
    install_pods
end


target 'socketIOTUITests' do
    use_frameworks!
    install_pods
end
